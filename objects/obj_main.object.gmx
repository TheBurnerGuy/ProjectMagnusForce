            <string>// Combat controls
if keyboard_check(ord('1')) {
    global.currentBallMode = ballMode.roll;
}
if keyboard_check(ord('2')) {
    global.currentBallMode = ballMode.trap;
}
if keyboard_check(ord('3')) {
    global.currentBallMode = ballMode.turret;
}

if mouse_check_button_pressed(mb_left) &amp;&amp; !immobile &amp;&amp; global.mainBall &gt; 0{
    global.mainBall -= 1;
    var newBall = instance_create(x, y, obj_ball);
    with(newBall) {
        self.mode = global.currentBallMode;
    }
}

if !recharging &amp;&amp; global.mainBall &lt; global.ballLimit {
    recharging = true;
    alarm_set(2, ballRecharge);
}
