        <arguments>
          <argument>
            <kind>1</kind>
            <string>// Movement modifiers
var acceleration = 1;
var deceleration = 2;
var standDeceleration = 0.5;
var sprintModifier = 1; // boolean for sprinting
var moveLimit = 10;
var energyRecharge = 3;
var imageWalkSpeed = 0.2;
var imageStandSpeed = 0;
var imageRunSpeed = 0.5;

// Movement variables
var moveX = 0;
var moveY = 0;

// Get keyboard input
if keyboard_check(ord('A')) {
    moveX -= 1;
}
if keyboard_check(ord('D')) {
    moveX += 1;
}
if keyboard_check(ord('W')) {
    moveY -= 1;
}
if keyboard_check(ord('S')) {
    moveY += 1;
}
if keyboard_check(vk_shift) {
    if(energy &gt; 0){
        energy -= 8;
        sprintModifier = 2;
        moveLimit = 15;
    }
}

// Immobile trumps all movement attempts
if (immobile) {
    moveX = 0;
    moveY = 0;
    movementX = 0;
    movementY = 0;
    image_alpha = 0.6;
} else {
    image_alpha = 1;
}

// Acceleration / Deceleration calculations
if (movementX * moveX &gt; 0 || (movementX == 0 &amp;&amp; moveX != 0)) { // accelerating
    movementX = min(moveLimit, ( abs(moveX) * acceleration * sprintModifier + abs(movementX) )) * moveX;
} else if (movementX * moveX &lt; 0) { // decelerating
    movementX += moveX * deceleration * sprintModifier;
} else {
    movementX = max(abs(movementX) - standDeceleration, 0) * sign(movementX);
}

if (movementY * moveY &gt; 0 || (movementY == 0 &amp;&amp; moveY != 0)) { // accelerating
    movementY = min(moveLimit, ( abs(moveY) * acceleration * sprintModifier + abs(movementY) )) * moveY;
} else if (movementY * moveY &lt; 0) { // decelerating
    movementY += moveY * deceleration * sprintModifier;
} else {
    movementY = max(abs(movementY) - standDeceleration, 0) * sign(movementY);
}

// Change sprites
var prev_sprite_index = sprite_index;
if abs(movementX) &gt;= abs(movementY) {
    if movementX &gt; 0 {
        sprite_index = spr_main_walk_r
        image_speed = imageWalkSpeed;
    } else if movementX &lt; 0 {
        sprite_index = spr_main_walk_l
        image_speed = imageWalkSpeed;
    } else {
        image_index = 0;
        image_speed = imageStandSpeed;
    }
} else {
    if movementY &gt; 0 {
        sprite_index = spr_main_walk_d
        image_speed = imageWalkSpeed;
    } else if movementY &lt; 0 {
        sprite_index = spr_main_walk_u
        image_speed = imageWalkSpeed;
    } else {
        image_index = 0;
        image_speed = imageStandSpeed;
    }
}
if sprintModifier &gt; 1 image_speed = imageRunSpeed;
if sprite_index != prev_sprite_index image_index = 0;

// Move and check for solid collision
if place_meeting(x + movementX, y, obj_wall) {
    if(moveX &gt; 0){
        move_contact_solid(0, moveLimit);
        movementX = 0;
    }else if(moveX &lt; 0){
        move_contact_solid(180, moveLimit);
        movementX = 0;
    }
} else {
    x += movementX;
}
if place_meeting(x, y + movementY, obj_wall) {
    if(moveY &gt; 0){
        move_contact_solid(270, moveLimit);
        movementY = 0;
    }else if(moveY &lt; 0){
        move_contact_solid(90, moveLimit);
        movementY = 0;
    }
} else {
    y += movementY;
}
</string>
